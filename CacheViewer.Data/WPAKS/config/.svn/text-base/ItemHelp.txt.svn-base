<BEGIN_TYPE>
TYPE= WARRANT

<BEGIN_SUBTYPE>
SUBTYPE=		0	#Blank Warrant
DESCRIPTOR=		"Type of warrant:  Blank"
DESCRIPTOR=		"This is a blank warrant, that has yet to be addressed to any"
DESCRIPTOR=		"specific individual, guild, or nation.  It will automatically"
DESCRIPTOR=		"be used if you drag a crest into your inventory while the "
DESCRIPTOR=		"warrant resides there."
<END_SUBTYPE>

<BEGIN_SUBTYPE>
SUBTYPE=		1	#Individual Warrant
DESCRIPTOR=		"Type of warrant:  Individual character"
DESCRIPTOR=		"This contains the crest corresponding to a specific person."
<END_SUBTYPE>

<BEGIN_SUBTYPE>
SUBTYPE=		2	#Guild Warrant
DESCRIPTOR=		"Type of warrant:  Guild"
DESCRIPTOR=		"This contains the crest corresponding to a guild."
<END_SUBTYPE>

<BEGIN_SUBTYPE>
SUBTYPE=		3	#Nation Warrant
DESCRIPTOR=		"Type of warrant:  Nation"
DESCRIPTOR=		"This contains the crest corresponding to a nation."
<END_SUBTYPE>

<BEGIN_SUBTYPE>
SUBTYPE=		4	#Full Warrant
DESCRIPTOR=		"Type of warrant:  Full"
DESCRIPTOR=		"This contains the crest corresponding to some combination of"
DESCRIPTOR=		"individual, guild, and nation."
<END_SUBTYPE>

POSTSCRIPT=		"Warrants are used to record the heraldry of both enemy and"
POSTSCRIPT=		"ally, and are frequently exchanged during the consumation"
POSTSCRIPT=		"of guild alliances.  Warrants that actively contain information"
POSTSCRIPT=		"can be added to any heraldry, kill on sight, or friends list"
POSTSCRIPT=		"simply by dragging the warrant object onto the given window."

<END_TYPE>

